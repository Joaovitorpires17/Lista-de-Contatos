generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

enum Gender {
  MASCULINO
  FEMININO
  NAO_BINARIO
  OUTRO 
}

model Contact {
  id                Int      @id @default(autoincrement())
  name              String
  email             String
  phone             String
  favorite          Boolean  @default(false)
  active            Boolean  @default(true)
  createdAt         DateTime @default(now())
  
  gender            Gender?   
  dateOfBirth       DateTime? 
  profilePictureUrl String?

}   


